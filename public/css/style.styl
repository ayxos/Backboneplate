    color #93a1a1
    padding 2%
  pre
    font-size 13px
    line-height 20px
    font-family Monaco,Menlo,Consolas,"Courier New",monospace;
    input,textarea
      border none
      width 20%
      margin-right:1%
      border: 1px solid #51b6ae;
    textarea
      width 90%
      height 100px
  .entry
    margin-left 5%
    .pln
      color #D14
      display inline-block
    .str
      color black
      font-wieght 600
      display inline-block
