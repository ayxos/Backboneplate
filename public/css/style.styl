body
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
  background ghostwhite
  margin 0

article
  padding: 50px
a
  color: #00B7FF

td
  padding 5px

.persona
  margin-bottom 3%

footer
  position fixed
  bottom 0%
  background black
  color white
  width 100%
  padding 1%

#entries
  margin-bottom 4%
  .entry
    margin 2%

.api
  h1
    background #D5D5D5
    padding 2%
    border 1px solid rgb(81, 182, 174)
